{
  "name": "stele",
  "version": "2.1.6",
  "private": true,
  "description": "Kiosk app wrapper for museum media exhibits",
  "author": "Science Museum of Minnesota <exhibit.media@smm.org>",
  "main": "./src/main/main.prod.js",
  "engines": {
    "node": "14.x"
  },
  "scripts": {
    "build": "concurrently \"yarn build-main\" \"yarn build-renderer\"",
    "build-dll": "cross-env NODE_ENV=development webpack --config ./config/webpack.config.renderer.dev.dll.babel.js --color",
    "build-e2e": "cross-env E2E_BUILD=true yarn build",
    "build-main": "cross-env NODE_ENV=production webpack --config ./config/webpack.config.main.prod.babel.js --color",
    "build-renderer": "cross-env NODE_ENV=production webpack --config ./config/webpack.config.renderer.prod.babel.js --color",
    "depcheck": "node -r esm scripts/depcheck.js",
    "install-app-deps": "electron-builder install-app-deps",
    "lint": "cross-env NODE_ENV=development eslint --cache --format=pretty .",
    "lint-fix": "yarn --silent lint --fix; exit 0",
    "lint-styles": "stylelint --ignore-path .eslintignore '**/*.*(css|scss)' --syntax scss",
    "lint-styles-fix": "yarn --silent lint-styles --fix; exit 0",
    "package": "yarn build && electron-builder build --publish never",
    "package-all": "yarn build && electron-builder build --mac --win --publish never",
    "package-all-publish": "yarn build && electron-builder build --mac --win --publish always",
    "package-ci": "yarn postinstall && yarn build && electron-builder --publish never",
    "package-mac": "yarn build && electron-builder build --mac",
    "package-publish": "yarn build && electron-builder build --publish always",
    "package-rpi": "yarn build && electron-builder build --linux --armv7l --publish never",
    "package-win": "yarn build && electron-builder build --win --x64",
    "postinstall": "yarn install-app-deps && yarn build-dll && ./node_modules/.bin/electron-rebuild && yarn depcheck",
    "prestart": "yarn build",
    "start": "cross-env START_HOT=1 node -r esm ./scripts/CheckPortInUse.js && cross-env START_HOT=1 yarn start:renderer",
    "start-delay": "cross-env START_HOT=1 node -r esm ./scripts/CheckPortInUse.js && cross-env START_HOT=1 STELE_DELAY=5 yarn start-renderer",
    "start:main": "cross-env HOT=1 NODE_ENV=development LOG_RENDER_CONSOLE=true run-electron -r esm ./src/main/main.dev.js",
    "start:renderer": "cross-env NODE_ENV=development webpack-dev-server --config config/webpack.config.renderer.dev.babel.js"
  },
  "dependencies": {
    "auto-launch": "^5.0.5",
    "bootstrap-css-only": "^4.4.1",
    "classnames": "^2.2.6",
    "core-js": "2",
    "devtron": "^1.4.0",
    "electron-localshortcut": "^3.1.0",
    "electron-log": "3.0.5",
    "electron-store": "^8.0.1",
    "formik": "^1.3.2",
    "popper.js": "^1.14.3",
    "prop-types": "^15.6.2",
    "react": "^16.6.3",
    "react-delay": "^0.1.0",
    "react-dom": "^16.6.3",
    "react-hot-loader": "^4.3.12",
    "react-icons": "^3.4.0",
    "react-router-dom": "5.0.0",
    "reactstrap": "^9.0.1",
    "run-electron": "1.0.0",
    "serialport": "^9.2.7",
    "source-map-support": "^0.5.9",
    "valid-url": "^1.0.9",
    "winston": "^3.1.0",
    "winston-daily-rotate-file": "^3.5.1",
    "yup": "0.32.11"
  },
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-decorators": "^7.1.6",
    "@babel/plugin-proposal-do-expressions": "^7.0.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-function-bind": "^7.0.0",
    "@babel/plugin-proposal-function-sent": "^7.1.0",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.0.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.0.0",
    "@babel/plugin-proposal-numeric-separator": "^7.0.0",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/plugin-proposal-pipeline-operator": "^7.0.0",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/plugin-transform-react-constant-elements": "^7.0.0",
    "@babel/plugin-transform-react-inline-elements": "^7.0.0",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.16.0",
    "react-refresh": "^0.10.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.3",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.1.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-dev-expression": "^0.2.1",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.20",
    "chalk": "^2.4.1",
    "clean-webpack-plugin": "^4.0.0",
    "concurrently": "^6.4.0",
    "cross-env": "^7.0.3",
    "crypto-browserify": "^3.12.0",
    "csp-html-webpack-plugin": "^5.1.0",
    "css-loader": "^6.5.1",
    "css-minimizer-webpack-plugin": "^3.0.2",
    "depcheck": "0.8.0",
    "detect-port": "^1.3.0",
    "electron": "^16.0.4",
    "electron-builder": "^22.14.5",
    "electron-debug": "^3.2.0",
    "electron-devtools-installer": "^3.2.0",
    "electron-rebuild": "^3.2.3",
    "eslint": "8.3.0",
    "eslint-config-airbnb": "^19.0.1",
    "eslint-formatter-pretty": "^4.1.0",
    "eslint-import-resolver-webpack": "0.13.2",
    "eslint-plugin-compat": "4.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-promise": "^5.1.1",
    "eslint-plugin-react": "^7.11.1",
    "esm": "^3.2.25",
    "file-loader": "^2.0.0",
    "html-loader": "^3.0.1",
    "html-webpack-plugin": "^5.5.0",
    "lodash": "^4.17.11",
    "mini-css-extract-plugin": "^2.4.5",
    "mocha": "^9.1.3",
    "optimize-css-assets-webpack-plugin": "^6.0.1",
    "path-browserify": "^1.0.1",
    "postcss-flexbugs-fixes": "^4.2.1",
    "postcss-loader": "3.0.0",
    "postcss-preset-env": "^6.7.0",
    "sass": "^1.43.4",
    "sass-loader": "^12.3.0",
    "spectron": "^15.0.0",
    "stream-browserify": "^3.0.0",
    "style-loader": "^0.23.1",
    "stylelint": "10.0.1",
    "stylelint-config-standard": "^18.2.0",
    "terser-webpack-plugin": "^1.1.0",
    "url-loader": "^1.1.2",
    "webpack": "^5.64.4",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.6.0",
    "webpack-merge": "^5.8.0",
    "yarn": "^1.12.3"
  },
  "build": {
    "productName": "Stele",
    "appId": "org.smm.stele",
    "artifactName": "${productName}-${os}-${version}.${ext}",
    "files": [
      "src/dist/",
      "src/renderer/index.html",
      "src/main/main.prod.js",
      "src/main/main.prod.js.map",
      "package.json"
    ],
    "linux": {
      "target": [
        "deb"
      ]
    },
    "deb": {
      "fpm": [
        "--architecture",
        "armhf"
      ]
    },
    "mac": {
      "target": [
        "dmg"
      ]
    },
    "dmg": {
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "win": {
      "target": [
        "nsis",
        "msi"
      ]
    },
    "nsis": {
      "artifactName": "${productName}-${os}-${version}-installer.${ext}"
    },
    "publish": [
      {
        "provider": "github",
        "owner": "scimusmn",
        "repo": "stele"
      }
    ],
    "directories": {
      "buildResources": "resources",
      "output": "release"
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
